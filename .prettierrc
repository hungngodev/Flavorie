{ 
    "plugins": ["prettier-plugin-organize-imports"],
    "organizeImports": true,
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": ["src/**/*.ts", "include/**/*.ts"],
        "options": {
          "parser": "typescript"
        }
      }
    ],
"organizeImportsSkipDestructiveCodeActions": true
 }