{
  "name": "flavorie",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "setup-project": "npm i && cd client && npm i",
    "setup-production-app": "npm i && cd client && npm i && npm run build && cd src && cd flask-server && pip install -r requirements.txt",
    "build": "npx tsc",
    "server": "nodemon --watch 'src/**/*' -e ts --exec \"node --no-warnings --loader ts-node/esm ./src/index.ts\" ",
    "client": "cd client && npm run dev",
    "peerjs": "cd peerjs && npm run dev",
    "flask-server": "cd src && cd flask-server && python3 run.py",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run flask-server\"   "
  },
